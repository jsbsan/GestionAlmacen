Public Sub GridViewConceptosEntrada_ColumnClick(Column As Integer)

  If var.ordenConceptosEntrada = 0 Then
    OrdenacionGridview.ord_AZ(GridViewConceptosEntrada, Column)
    var.ordenConceptosEntrada = 1
  Else
    var.ordenConceptosEntrada = 0
    OrdenacionGridview.ord_ZA(GridViewConceptosEntrada, Column)
  Endif

End

